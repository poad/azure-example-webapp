{
  "name": "next-ts-minimal-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "echo ''",
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "elint-check": "next lint --ext .ts,.js,.tsx,.jsx src",
    "elint-fix": "next lint --fix --ext .ts,.js,.tsx,.jsx src",
    "prettier-check": "prettier --check src",
    "prettier-format": "prettier --write src",
    "lint": "pnpm prettier-check && pnpm elint-check",
    "lint-fix": "pnpm prettier-format && pnpm elint-fix",
    "clean": "rimraf build",
    "all": "pnpm lint && pnpm build && pnpm test"
  },
  "dependencies": {
    "@next/font": "^13.5.6",
    "next": "^13.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.5.6",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.57",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-next": "^13.5.6",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "next-transpile-modules": "^10.0.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.5",
    "swc-loader": "^0.2.6",
    "typescript": "^5.3.3"
  }
